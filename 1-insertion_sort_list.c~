#include <stdio.h>
#include "sort.h"




void insertion_sort_list(listint_t **list)
{
listint_t *curr, *next;
if (list == NULL || *list == NULL || (*list)->next == NULL)
{
return;
}
curr = (*list)->next;
while (current != NULL)
{
next = curr->next;
while (curr->prev != NULL && curr->n < curr->prev->n)
{
curr->prev->next = curr->next;
if (curr->next != NULL)
{
curr->next->prev = curr->prev;
}
}
}
}
